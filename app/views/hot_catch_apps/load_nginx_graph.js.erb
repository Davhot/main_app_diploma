var chart;

<% if @graph_data_x.count > 0 %>
  <% @graph_data_x.unshift('x'); @graph_data_y.unshift('Nginx') %>
  // console.log(<%== @graph_data_x %>)
  chart = c3.generate({
    point: {
      r: 5
    },
    bindto: '#nginx-chart',
    data: {
      x: 'x',
      xFormat: '<%== @parse_c3_date_format %>',
      columns: [<%== @graph_data_x %>, <%== @graph_data_y %>],
      onclick: function(d, element) {
        var find_date;
        $("#modal-nginx-logs").modal('show');
        find_date = moment(d.x).format('<%== @moment_format %>');
        return $.ajax({
          url: '<%= @nginx_logs_path %>' + "?nginx-date=" + find_date
        }).done(function(data) {
          var i, j, len, nginx_log, ref, results;
          if (data.status) {
            $("#modal-nginx-logs-body").html("");
            ref = data.nginx_logs;
            results = [];
            for (i = j = 0, len = ref.length; j < len; i = ++j) {
              nginx_log = ref[i];
              results.push($("#modal-nginx-logs-body").append("" + nginx_log.log));
            }
            return results;
          }
        });
      }
    },
    axis: {
      x: {
        type: 'timeseries',
        localtime: true,
        tick: {
          count: 8,
          format: '<%== @show_datetime_format %>'
        }
      }
    },
    subchart: {
      show: true
    },
    zoom: {
      enabled: true,
      extent: [1, 100]
    },
    legend: {
      hide: 'Nginx'
    }
  });
<% end %>
